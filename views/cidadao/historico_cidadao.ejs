<%- include('../layouts/header') %>
<body id="layoutPadrao" id="tela">
    <!--CABEÇALHO-->
    <div id="cabecalho">
        <header>
            <div style="display:flex; justify-content:center; align-items:center;" id="titulo_cotainer">
                <h1>HISTÓRICO DE AÇÕES</h1>
            </div>
            <p><%= nome %></p>
        </header>


        <div class="voltar"><a href="/"><i class="fa-solid fa-house"></i></a></div>


        <!--Histórico-->
        <main class="historico">
            <div class="historico_container" id="historico_container">
                <div class="id_historico"><p id="cpf_historico"></p></div>
                <input type="text" value="<%=cpf%>" id="cpf" style="display: none!important" readonly>
                <h3 id="mes"></h3>

                <div class="containerTable" id="tabela">
                    <img src="img/scroll.gif" alt="" class="scroll-icon" id="scrollIcon">
                    <table>
                        <thead>
                            <tr>
                                <th>N°</th>
                                <th>DATA/HORA</th>
                                <th><img src="/img/imgs_checklist/img1.jpg" alt="" width="20px"></th>
                                <th><img src="/img/imgs_checklist/img2.jpg" alt="" width="20px"></th>
                                <th><img src="/img/imgs_checklist/img3.jpg" alt="" width="20px"></th>
                                <th><img src="/img/imgs_checklist/img4.jpg" alt="" width="20px"></th>
                                <th><img src="/img/imgs_checklist/img5.jpg" alt="" width="20px"></th>
                                <th><img src="/img/imgs_checklist/img6.jpg" alt="" width="20px"></th>
                                <th><img src="/img/imgs_checklist/img7.jpg" alt="" width="20px"></th>
                                <th><img src="/img/imgs_checklist/img8.jpg" alt="" width="20px"></th>
                                <th><img src="/img/imgs_checklist/img9.jpg" alt="" width="20px"></th>
                                <th><img src="/img/imgs_checklist/img10.jpg" alt="" width="20px"></th>
                                <th><img src="/img/imgs_checklist/img11.jpg" alt="" width="20px"></th>
                            </tr>
    
                        </thead>
    
                        <tbody id="corpo_historico">
                            
                        </tbody>
                    </table>
                </div>
            </div>
    
            
            
    
            <nav class="backContainer">
                <form action="/ir_menu_cidadao" method="POST">
                    <input type="text" name="nome_cidadao" value="<%=nome%>" style="display: none!important" readonly>
                    <input type="text" name="cpf_cidadao" value="<%=cpf%>" style="display: none!important" readonly>
                    <button type="submit">
                        <i class="fa-solid fa-arrow-left"></i>
                    </buton> 
                </form>  
            </nav>
        </main>
    </div>






   



    <!--RODAPÉ-->
    <footer>
        <div>
            <img src="/img/logo-prefeitura.png" alt="Logomarca prefeitura" width="300px" style="margin-top: 20px;">
            <p style="margin-top: 10px; color:black;">Xô Aedes</p>
        </div>
    </footer>
</body>

<script type="module" src="/js/scripts_cidadao/historico_cidadao.js"></script>
<script type="module" src="/js/scripts_cidadao/scroll.js"></script>
<%- include('../layouts/footer') %>